{
    "name": "@xweather/maps-ui-sdk",
    "version": "1.0.5",
    "license": "MIT",
    "author": "Xweather",
    "homepage": "https://www.xweather.com",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./style": "./dist/style.css",
        "./dist/style.css": "./dist/style.css"
    },
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "style": "./dist/style.css",
    "engines": {
        "node": ">=18.7.0",
        "npm": ">=7.9.1"
    },
    "scripts": {
        "prepare": "husky install",
        "dev": "TS_CONFIG=tsconfig.json vite ./example",
        "build:analyze": "vite build --config vite.analyze.ts",
        "build:ts": "tsc -p tsconfig-esm.json && tsc-alias -p tsconfig-esm.json",
        "build:css": "postcss src/index.css -o dist/style.css",
        "build": "yarn clean && yarn build:ts && yarn build:css",
        "dev:example": "vite ./example",
        "build:example": "vite build ./example",
        "clean": "rm -fr dist",
        "preview": "vite preview",
        "lint": "yarn lint:script",
        "lint:eslint": "eslint --ext .ts,.tsx --config .eslintrc.cjs \"src/**/*.{ts,tsx}\"",
        "lint:fix": "yarn lint:eslint --fix",
        "lint:script": "eslint --ext .ts,tsx --ignore-path .gitignore .",
        "lint:style": "stylelint src/**/*.{css,scss}",
        "tsc:check": "tsc",
        "commit": "cz",
        "prepublishOnly": "yarn run clean && yarn run build",
        "prepush": "yarn run build"
    },
    "dependencies": {
        "@aerisweather/javascript-utils": "^0.5.9",
        "@radix-ui/react-select": "^2.1.6",
        "@radix-ui/react-slider": "^1.2.2",
        "@radix-ui/react-tooltip": "^1.1.6",
        "chroma-js": "^3.1.2",
        "clsx": "^2.1.0",
        "date-fns": "3.0.0",
        "date-fns-tz": "3.1.3",
        "deepmerge-ts": "^7.1.5",
        "framer-motion": "^10.16.4",
        "nanoid": "^5.0.9"
    },
    "peerDependencies": {
        "@aerisweather/mapsgl": "^1.7.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@aerisweather/eslint-plugin": "^1.0.4",
        "@aerisweather/mapsgl": "^1.7.3",
        "@commitlint/cli": "^19.2.1",
        "@commitlint/config-conventional": "^19.1.0",
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-strip": "^3.0.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/chroma-js": "^2.4.4",
        "@types/node": "*",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "autoprefixer": "^10.4.20",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.52.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unicorn": "^51.0.1",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "postcss": "^8.4.31",
        "postcss-cli": "^11.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rollup-plugin-typescript-paths": "^1.5.0",
        "rollup-plugin-visualizer": "^5.12.0",
        "stylelint": "^14.12.1",
        "stylelint-config-recommended": "^9.0.0",
        "stylelint-config-standard": "^28.0.0",
        "tailwindcss": "^3.3.3",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.2.2",
        "vite": "^5.4.10",
        "vite-plugin-circular-dependency": "^0.3.1",
        "vite-plugin-dts": "^4.5.0"
    },
    "lint-staged": {
        "*.{ts,tsx}": "eslint --fix",
        "*.{css, scss}": "stylelint --fix"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "files": [
        "dist",
        "example",
        "package.json",
        "README.md",
        "LICENSE"
    ]
}
